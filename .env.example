
# ==================================
# Application
# ==================================
NODE_ENV=production
VITE_APP_VERSION=2.0.0
VITE_APP_NAME=AstroLeads

# ==================================
# Supabase
# ==================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# ==================================
# AI Services
# ==================================
VITE_GEMINI_API_KEY=your-gemini-key-here
VITE_OPENROUTER_API_KEY=your-openrouter-key

# ==================================
# Monitoring & Analytics
# ==================================
VITE_SENTRY_DSN=https://xxx@sentry.io/xxx
VITE_POSTHOG_KEY=phc_xxxx
VITE_MIXPANEL_TOKEN=xxxx

# ==================================
# Feature Flags
# ==================================
VITE_FEATURE_AI_STUDIO=true
VITE_FEATURE_ADVANCED_ANALYTICS=true
VITE_FEATURE_TEMPLATE_LIBRARY=true

# ==================================
# Security
# ==================================
REDIS_PASSWORD=your-redis-password
GRAFANA_PASSWORD=your-grafana-password

# ==================================
# CI/CD
# ==================================
SNYK_TOKEN=your-snyk-token
CODECOV_TOKEN=your-codecov-token
RAILWAY_STAGING_WEBHOOK=https://xxx
RAILWAY_PRODUCTION_WEBHOOK=https://xxx
